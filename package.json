{
  "name": "ng-vcl-plotly",
  "version": "0.1.0",
  "description": "",
  "main": "plotly.component.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/plotly.git"
  },
  "homepage": "https://github.com/ng-vcl",
  "license": "MIT",
  "author": "The NG VCL authors",
  "contributors": [
    {
      "name": "Michael Neuhold",
      "email": "michael.neuhold@n-fuse.de"
    }
  ],
  "scripts": {
    "postinstall":"jspm install plotly=github:plotly/plotly.js -o \"{main:'dist/plotly.js'}\""
  },
  "devDependencies": { },
  "keywords": [
    "web-component",
    "angular2",
    "VCL"
  ],
  "jspm": {
    "dependencies": {
      "plotly": "github:plotly/plotly.js@1.15.0",
      "ts": "github:frankwallis/plugin-typescript@^5.0.0",
      "text": "github:systemjs/plugin-text@^0.0.8"
    },
    "peerDependencies": {
      "@angular/core": "npm:@angular/core@^2.0.0-rc.4"
    },
    "meta": {
      "*.ts": {
        "loader": "ts"
      },
      "*.html": {
        "loader": "text"
      }
    },
    "overrides": {
      "github:plotly/plotly.js@1.15.0": {
        "main": "dist/plotly.js"
      },
      "npm:@angular/core@2.0.0-rc.4": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      }
    }
  }
}
